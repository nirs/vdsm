#!/bin/sh
if [ -z "$PYTHON_EXE" ]; then
    PYTHON_EXE="python2"
fi

prefix="@prefix@"

export \
    LC_ALL=C \
    NOSE_LOGFORMAT='%(asctime)s %(levelname)-7s [%(name)s] (%(threadName)s) %(message)s' \
    NOSE_VERBOSE=${NOSE_VERBOSE:-3} \
    PYTHONPATH="@datarootdir@/vdsm" \
    $NULL

"$PYTHON_EXE" @top_srcdir@/tests/testrunner.py $@
